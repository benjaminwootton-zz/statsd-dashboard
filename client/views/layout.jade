doctype 5
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(type='text/javascript', src='https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js')
    script(type='text/javascript', src='/flot/jquery.flot.js')
    script(type='text/javascript', src='/socket.io.js')
    script(type='text/javascript', src='/stats-dashboard.js')    
  body
    block content
    div#messages
    
    h2 Counters 

    table#countertable(class='dashboardtable')
        thead 
            tr
                th Key
                th Value 
                th Chart
        tbody

    h2 Timers 

    table#timertable(class='dashboardtable')
        thead 
            tr
                th Key
                th Upper
                th Lower
                th Count
                th Sum
                th Mean
                th Chart
        tbody
        

 